{
  "expo": {
    "name": "Travo",
    "slug": "travo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "travo",
    "userInterfaceStyle": "automatic",
    "updates": {
      "enabled": false
    },
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.utkarsh07.travo",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Travo needs your location to show you on the map and find nearby interests.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Travo uses your location to keep your journey updated for your friends, even in the background.",
        "NSLocationAlwaysUsageDescription": "Travo uses your location to keep your journey updated for your friends."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "config": {
        "googleMaps": {
          "apiKey": "process.env.GOOGLE_MAPS_API_KEY"
      }},
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "package": "com.utkarsh07.travo"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f2a1a98a-e70c-48ed-9a52-b7ae8b8cd49f"
      }
    },
    "owner": "utkarsh_07"
  }
}
